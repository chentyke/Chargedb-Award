{
  "name": "chargedb-award",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:client\"",
    "dev:host": "concurrently \"npm:dev:server\" \"npm:dev:client -- --host 0.0.0.0\"",
    "dev:client": "vite",
    "dev:server": "nodemon --watch server --ext js,json --exec node server/index.js",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node server/index.js"
  },
  "dependencies": {
    "@notionhq/client": "^2.2.15",
    "@tanstack/react-query": "^5.59.20",
    "@tanstack/react-router": "^1.90.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "framer-motion": "^12.23.26",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.2",
    "concurrently": "^9.0.1",
    "nodemon": "^3.1.4",
    "typescript": "^5.5.4",
    "vite": "^7.3.0"
  }
}
